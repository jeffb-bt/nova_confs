[DEFAULT]
logdir = /var/log/nova
state_path = /var/lib/nova
lock_path = /var/lib/nova/tmp
volumes_dir = /etc/nova/volumes
dhcpbridge = /usr/bin/nova-dhcpbridge
dhcpbridge_flagfile = /etc/nova/nova.conf
force_dhcp_release = True
injected_network_template = /usr/share/nova/interfaces.template
libvirt_nonblocking = True
libvirt_inject_partition = -1
iscsi_helper = tgtadm
sql_connection = mysql://nova:xxxxxxxxxxxxxxxxxx@10.5.2.1/nova
compute_driver = libvirt.LibvirtDriver
firewall_driver = nova.virt.libvirt.firewall.IptablesFirewallDriver
rpc_backend = nova.openstack.common.rpc.impl_qpid
rootwrap_config = /etc/nova/rootwrap.conf
flat_interface=eth1
qpid_reconnect_interval=0
qpid_hostname=10.5.2.1
vncserver_proxyclient_address=10.5.2.3
qpid_reconnect_interval_min=0
metadata_host=10.5.2.1
qpid_reconnect=true
glance_api_servers=10.5.2.1:9292
qpid_username=guest
verbose=false
vnc_enabled=true
qpid_password=xxxxxxxxxxxxxxxxx
qpid_reconnect_timeout=0
image_service=nova.image.glance.GlanceImageService
volume_api_class=nova.volume.cinder.API
qpid_reconnect_interval_max=0
qpid_heartbeat=60
novncproxy_base_url=http://10.5.2.1:6080/vnc_auto.html
qpid_reconnect_limit=0
service_down_time=60
qpid_protocol=tcp
vncserver_listen=0.0.0.0
auth_strategy=keystone
qpid_tcp_nodelay=true
qpid_port=5672
libvirt_type=kvm
connection_type=libvirt
quantum_auth_strategy=keystone
network_api_class=nova.network.quantumv2.api.API
quantum_admin_auth_url=http://10.5.2.1:35357/v2.0
quantum_admin_password=xxxxxxxxxxxxxxxxxxxxx
quantum_admin_username=quantum
quantum_admin_tenant_name=services
quantum_url=http://10.5.2.1:9696

[keystone_authtoken]
admin_tenant_name = %SERVICE_TENANT_NAME%
admin_user = %SERVICE_USER%
admin_password = %SERVICE_PASSWORD%
auth_host = 127.0.0.1
auth_port = 35357
auth_protocol = http
signing_dir = /tmp/keystone-signing-nova

